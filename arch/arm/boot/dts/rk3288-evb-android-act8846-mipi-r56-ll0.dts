/dts-v1/;
#include "rk3288-evb-r56.dtsi"
#include "rk3288-android.dtsi"
#include <dt-bindings/sensor-dev.h>

/ {
	compatible = "rockchip,rk3288-evb-act8846", "rockchip,rk3288";

	vcc_lcd: vcc-lcd {
		compatible = "regulator-fixed";
		regulator-boot-on;		
		enable-active-low;
		regulator-always-on;
		gpio = <&gpio7 11 GPIO_ACTIVE_LOW>;
		pinctrl-names = "default";
		pinctrl-0 = <&lcd_en>;
		regulator-name = "vcc_lcd";
		vin-supply = <&vcc_io>;
	};

	xin32k: xin32k {
		compatible = "fixed-clock";
		clock-frequency = <32768>;
		clock-output-names = "xin32k";
		#clock-cells = <0>;
	};
};

&backlight {
	pwms = <&pwm0 0 500000 0>;
	enable-gpios = <&gpio7 9 GPIO_ACTIVE_HIGH>;
	resetusbhub-gpios = <&gpio8 3 GPIO_ACTIVE_HIGH>;
	post-pwm-on-delay-ms = <200>;
	pwm-off-delay-ms = <200>;
};

&cpu0 {
	cpu-supply = <&vdd_cpu>;
};

&rkisp1 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&isp_mipi &isp_dvp_d2d9 &isp_dvp_d0d1>;

	port {
		#address-cells = <1>;
		#size-cells = <0>;
		//modify by pirlo for gc2155 front camera
		dvp_in_fcam: endpoint@0 {
			reg = <0>;
			remote-endpoint = <&gc2155_out>;
		};
	};
};

/*&dfi {
	status = "okay";
};

&dmc {
	center-supply = <&vdd_log>;
	status = "okay";
};*/

/*
***TV080WXT-LL0******
*/
&dsi0 {
	status = "okay";
	rockchip,lane-rate = <1152>; //1130

	panel@0 {
		compatible = "simple-panel-dsi";
		reg = <0>;
		backlight = <&backlight>;
		enable-gpios = <&gpio7 3 GPIO_ACTIVE_HIGH>;
		reset-gpios = <&gpio7 10 GPIO_ACTIVE_LOW>;
		power-supply = <&vcc_lcd>;
		dsi,flags =  <(MIPI_DSI_MODE_VIDEO | MIPI_DSI_MODE_VIDEO_BURST | MIPI_DSI_MODE_LPM | MIPI_DSI_MODE_EOT_PACKET)>;
		dsi,format = <MIPI_DSI_FMT_RGB888>;
		bus-format = <MEDIA_BUS_FMT_RBG888_1X24>;
		dsi,lanes = <4>;
		reset-delay-ms = <30>;
		init-delay-ms = <30>;
		enable-delay-ms = <30>;
		prepare-delay-ms = <20>;
		unprepare-delay-ms = <30>; 
		status = "okay";
		
/*
	panel-init-sequence = [
				05 96 01 11
				05 32 01 29 				
		    ];
	panel-exit-sequence=[
				05 16 01 28
				05 78 01 10
		];	
*/	
		disp_timings: display-timings {
		native-mode = <&timing0>;
		timing0: timing0 {			
				clock-frequency = <60000000>;
				hactive = <800>;
				vactive = <1280>;
				hback-porch = <12>;
				hfront-porch = <12>;
				vback-porch = <32>;
				vfront-porch = <155>;
				hsync-len = <8>;
				vsync-len = <8>;
				hsync-active = <0>;
				vsync-active = <0>;
				de-active = <0>;
				pixelclk-active = <0>;
			};
		};	    
	};
};

&route_dsi0 {
	status = "okay";
	connect = <&vopl_out_dsi0>;
};

&dsi0_in_vopl {
        status = "okay";
};

&dsi0_in_vopb {
        status = "disabled";
};

&gpu {
	status = "okay";
	mali-supply = <&vdd_gpu>;
};

&gmac {
	max-speed = <1000>;
	status = "okay";
};

&hdmi_analog_sound {
	status = "okay";
};

&sound {
	status = "okay";
};

&i2c0 {
	clock-frequency = <400000>;

	vdd_cpu: syr827@40 {
		compatible = "silergy,syr827";
		fcs,suspend-voltage-selector = <1>;
		reg = <0x40>;
		regulator-name = "vdd_cpu";
		regulator-min-microvolt = <850000>;
		regulator-max-microvolt = <1350000>;
		regulator-always-on;
		regulator-boot-on;
		regulator-enable-ramp-delay = <300>;
		regulator-ramp-delay = <8000>;
		vin-supply = <&vcc_sys>;
		regulator-state-mem {
			regulator-off-in-suspend;
		};
	};

	vdd_gpu: syr828@41 {
		compatible = "silergy,syr828";
		fcs,suspend-voltage-selector = <1>;
		reg = <0x41>;
		regulator-name = "vdd_gpu";
		regulator-min-microvolt = <850000>;
		regulator-max-microvolt = <1350000>;
		regulator-always-on;
		regulator-ramp-delay = <6000>;
		vin-supply = <&vcc_sys>;
		regulator-state-mem {
			regulator-off-in-suspend;
		};
	};

	hym8563: hym8563@51 {
		compatible = "rtc,hym8563";
		reg = <0x51>;		
		mcu-io-init = <&gpio7 RK_PA4 GPIO_ACTIVE_HIGH>;	
		mcu-io-det = <&gpio7 RK_PA2 GPIO_ACTIVE_HIGH>;	
		status = "okay";		
	};

	act8846: act8846@5a {
		compatible = "active-semi,act8846";
		reg = <0x5a>;
		status = "okay";

		vp1-supply = <&vcc_sys>;
		vp2-supply = <&vcc_sys>;
		vp3-supply = <&vcc_sys>;
		vp4-supply = <&vcc_sys>;
		inl1-supply = <&vcc_io>;
		inl2-supply = <&vcc_sys>;
		inl3-supply = <&vcc_20>;

		regulators {
			vcc_ddr: REG1 {
				regulator-name = "VCC_DDR";
				regulator-min-microvolt = <1200000>;
				regulator-max-microvolt = <1200000>;
				regulator-always-on;
			};

			vcc_io: REG2 {
				regulator-name = "VCC_IO";
				regulator-min-microvolt = <3300000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			vdd_log: REG3 {
				regulator-name = "VDD_LOG";
				regulator-min-microvolt = <1150000>;
				regulator-max-microvolt = <1150000>;
				regulator-always-on;
			};

			vcc_20: REG4 {
				regulator-name = "VCC_20";
				regulator-min-microvolt = <2000000>;
				regulator-max-microvolt = <2000000>;
				regulator-always-on;
			};

			vccio_sd: REG5 {
				regulator-name = "VCCIO_SD";
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			vdd10_lcd: REG6 {
				regulator-name = "VDD10_LCD";
				regulator-min-microvolt = <1000000>;
				regulator-max-microvolt = <1000000>;
				regulator-always-on;
			};

			vcca_codec: REG7 {
				regulator-name = "VCCA_CODEC";
				regulator-min-microvolt = <3300000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			vcca_tp: REG8 {
				regulator-name = "VCCA_TP";
				regulator-min-microvolt = <3300000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			vccio_pmu: REG9 {
				regulator-name = "VCCIO_PMU";
				regulator-min-microvolt = <3300000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			vdd_10: REG10 {
				regulator-name = "VDD_10";
				regulator-min-microvolt = <1000000>;
				regulator-max-microvolt = <1000000>;
				regulator-always-on;
			};

			vcc_18: REG11 {
				regulator-name = "VCC_18";
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <1800000>;
				regulator-always-on;
			};

			vcc18_lcd: REG12 {
				regulator-name = "VCC18_LCD";
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <1800000>;
				regulator-always-on;
			};
		};
	};
};

&i2c1 {
	status = "okay";
	clock-frequency = <400000>;

	bma2x2: bma250@18 {
		status = "okay";
		compatible = "bma2xx_acc";
		reg = <0x18>;
		type = <SENSOR_TYPE_ACCEL>;
		irq-gpio = <&gpio8 7 IRQ_TYPE_LEVEL_LOW>;              /* GSENSOR_INT = GPIO8_A7 */
		irq_enable = <0>;
		poll_delay_ms = <30>;
		layout = <5>;  
    };
	
	kxtj9@0e {
		status = "okay";
		compatible = "gs_kxtj9";
		reg = <0x0e>;
		type = <SENSOR_TYPE_ACCEL>;
		irq_enable = <0>;
		poll_delay_ms = <20>;
		layout = <5>;
    };
};

&i2c2 {
	status = "okay";
	clock-frequency = <400000>;

	es8316: es8316@10 {
		status = "okay";
		compatible = "everest,es8316";
		reg = <0x10>;
		spk-con-gpio = <&gpio7 8 GPIO_ACTIVE_HIGH>;
		//hp-det-gpio = <&gpio7 7 GPIO_ACTIVE_HIGH>;
		hp-con-gpio = <&gpio0 13 GPIO_ACTIVE_HIGH>;		
		spk_volume = <0x99>;   //0x88 1.12w	0x99 1.5w

		clock-names = "mclk";
		clocks = <&cru SCLK_I2S0_OUT>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2s0_mclk>;
		#sound-dai-cells = <0>;
		extcon = <&rk_headset>;
	};
