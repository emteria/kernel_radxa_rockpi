	nfc-bcm2079x@76 {
                compatible = "nfc-bcm2079x.ap6441";
                reg = <0x76>;
				en_gpio = <&gpio7 14 GPIO_ACTIVE_HIGH>;
				wake_gpio = <&gpio7 15 GPIO_ACTIVE_HIGH>;
				irq_gpio = <&gpio0 1 IRQ_TYPE_EDGE_RISING>;
                status = "okay";
    };
*/
};

&i2c3 {
	status = "okay";

	gc2155@3c {
		status = "okay";
		compatible = "galaxycore,gc2155";
		reg = <0x3c>;
		//device_type = "v4l2-i2c-subdev";
		//pinctrl-names = "default";
		//pinctrl-0 = <&fcam_pd>;

		clocks = <&cru SCLK_VIP_OUT>;
		clock-names = "xvclk";

		power-gpio = <&gpio8 RK_PB1 GPIO_ACTIVE_HIGH>;
                pwdn-gpios = <&gpio2 RK_PB7 GPIO_ACTIVE_HIGH>;
		//pwdn-gpios = <&gpio2 RK_PB6 GPIO_ACTIVE_HIGH>;
		rockchip,camera-module-index = <1>;
		rockchip,camera-module-facing = "front";
		//rockchip,camera-module-name = "default";
		//rockchip,camera-module-lens-name = "default";
		rockchip,camera-module-name = "CameraKing";
		rockchip,camera-module-lens-name = "Largan";
		port {
			gc2155_out: endpoint {
				remote-endpoint = <&dvp_in_fcam>;
			};
		};
	};
};

&i2c4 {
	status = "okay";


	gt9xx: gt9xx@14 {
		compatible = "goodix,gt9xx";
		reg = <0x14>;
		touch-gpio = <&gpio7 6 IRQ_TYPE_LEVEL_LOW>;
		reset-gpio =<&gpio7 5 GPIO_ACTIVE_HIGH>;
		max-x = <1280>;
		max-y = <800>;
		tp-size = <89>;
		tp-supply = <&vcca_tp>;
		status = "okay";
	};
	
