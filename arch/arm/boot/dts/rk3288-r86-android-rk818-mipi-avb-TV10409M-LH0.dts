/dts-v1/;
#include "rk3288-r86-android-rk818-mipi.dtsi"

/ {
	model = "Rockchip rk3288 evb avb board";
	compatible = "rockchip,rk3288-evb-android-rk818-mipi", "rockchip,rk3288w";
	chosen: chosen {
		bootargs = "earlycon=uart8250,mmio32,0xff690000 vmalloc=496M console=ttyFIQ0 androidboot.baseband=N/A androidboot.veritymode=enforcing androidboot.hardware=rk30board androidboot.console=ttyFIQ0 init=/init kpti=0";
	};
};

&hevc {
	status = "okay";
	clock-names = "aclk_vcodec", "hclk_vcodec", "clk_core", "clk_cabac";
	rockchip,normal-rates = <300000000>, <0>, <200000000>, <200000000>;
	rockchip,advanced-rates = <600000000>, <0>, <500000000>, <500000000>;
};

&io_domains {
	status = "okay";

	dvp-supply = <&vcc_18>;//vcc18_lcd//ldo output
};

&isp_mmu {
	status = "okay";
};

&i2c2 {
	status = "okay";
	i2c-scl-rising-time-ns = <140>;
	i2c-scl-falling-time-ns = <30>;
	clock-frequency = <400000>;

	es8316: es8316@10 {
		#sound-dai-cells = <0>;
		compatible = "everest,es8316";
		reg = <0x10>;
		clocks = <&cru SCLK_I2S0_OUT>;
		clock-names = "mclk";
		pinctrl-names = "default";
		pinctrl-0 = <&i2s0_mclk>;
		//pinctrl-1 = <&hp_det>;
		//pinctrl-2 = <&hp_con>;	
        spk-con-gpio = <&gpio7 8 GPIO_ACTIVE_HIGH>;
		//hp-det-gpio = <&gpio4 RK_PD4 GPIO_ACTIVE_HIGH>; 	
        //hp-con-gpio = <&gpio7 9 GPIO_ACTIVE_HIGH>;
		spk_volume = <0xbb>;		
		extcon = <&rk_headset>;
		status = "okay";		
	};

	nfc-bcm2079x@76 {
		compatible = "nfc-bcm2079x.ap6441";
		reg = <0x76>;
		//pw_gpio = <&gpio7 21 GPIO_ACTIVE_HIGH>;//gpio7_c5
		//pw_gpio = <&gpio7 16 GPIO_ACTIVE_HIGH>;//gpio7_c0 
		en_gpio = <&gpio8 8 GPIO_ACTIVE_HIGH>;//gpio7_b6
		wake_gpio = <&gpio0 14 GPIO_ACTIVE_HIGH>;//gpio7_b7
		irq_gpio = <&gpio0 1 IRQ_TYPE_EDGE_RISING>;//gpio0_a1
		status = "disabled";
    };
};

&i2c3 {
	status = "okay";

	sgm3784: sgm3784@30 {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "sgmicro,gsm3784";
		reg = <0x30>;
		rockchip,camera-module-index = <0>;
		rockchip,camera-module-facing = "back";
		enable-gpio = <&gpio7 RK_PB6 GPIO_ACTIVE_HIGH>;
		strobe-gpio = <&gpio7 RK_PB5 GPIO_ACTIVE_HIGH>;
		status = "disabled";
		sgm3784_led0: led@0 {
			reg = <0x0>;
			led-max-microamp = <299200>;
			flash-max-microamp = <1122000>;
			flash-max-timeout-us = <1600000>;
		};

		sgm3784_led1: led@1 {
			reg = <0x1>;
			led-max-microamp = <299200>;
			flash-max-microamp = <1122000>;
			flash-max-timeout-us = <1600000>;
		};
	};

	vm149c: vm149c@0c {
		compatible = "silicon touch,vm149c";
		status = "disabled";
		reg = <0x0c>;
		rockchip,camera-module-index = <0>;
		rockchip,camera-module-facing = "back";
	};

	ov13850: ov13850@10 {
		status = "disabled";
		compatible = "ovti,ov13850";
		reg = <0x10>;
		clocks = <&cru SCLK_VIP_OUT>;
		clock-names = "xvclk";
		/* avdd-supply = <>; */
		/* dvdd-supply = <>; */
		/* dovdd-supply = <>; */
		/* reset-gpios = <>; */
		pinctrl-names = "rockchip,camera_default";
		pinctrl-0 = <&isp_mipi>;
		power-gpio = <&gpio0 RK_PC1 GPIO_ACTIVE_HIGH>;
		pwdn-gpios = <&gpio2 RK_PB7 GPIO_ACTIVE_HIGH>;
		rockchip,camera-module-index = <0>;
		rockchip,camera-module-facing = "back";
		rockchip,camera-module-name = "CMK-CT0116";
		rockchip,camera-module-lens-name = "Largan-50013A1";
		lens-focus = <&vm149c>;
		flash-leds = <&sgm3784_led0 &sgm3784_led1>;
		port {
			ov13850_out: endpoint {
				remote-endpoint = <&mipi_in_ucam0>;
				data-lanes = <1 2>;
			};
		};
	};
	
	gc5025: gc5025@37 {
		compatible = "galaxycore,gc5025";
		status = "okay";
		reg = <0x37>;
		//clocks = <&cru SCLK_CIF_OUT>;
		clocks = <&cru SCLK_VIP_OUT>;
		clock-names = "xvclk";
		/* avdd-supply = <>; */
		/* dvdd-supply = <>; */
		/* dovdd-supply = <>; */
		/* reset-gpios = <>; */
		//dovdd-supply = <&vcc1v8_dvp>;          
		//reset-gpios = <&gpio2 0 GPIO_ACTIVE_HIGH>; 
		power-gpio = <&gpio7 RK_PC5 GPIO_ACTIVE_HIGH>;
        pwdn-gpios = <&gpio2 14 GPIO_ACTIVE_LOW>;
		pinctrl-names = "rockchip,camera_default";
		pinctrl-0 = <&isp_mipi>;		
		rockchip,camera-module-index = <0>;
		rockchip,camera-module-facing = "front";
		rockchip,camera-module-name = "CMK-CW4191-FG1";
		rockchip,camera-module-lens-name = "CK5502";                

		port {
				ucam_out0: endpoint {
						remote-endpoint = <&mipi_in_ucam0>;
						//remote-endpoint = <&mipi_in_ucam1>;
						data-lanes = <1 2>;
				};
		};
    }; 
	
};

&i2c4 {
	status = "okay";
	
	himax@48 {
		status = "okay";
		compatible = "himax,hxcommon";
		reg = <0x48>;
		himax,panel-coords = <0 2000 0 1200>;
		himax,display-coords = <0 2000 0 1200>;
		himax,irq-gpio = <&gpio7 6 IRQ_TYPE_EDGE_FALLING>;  	
	 	himax,rst-gpio = <&gpio7 5 GPIO_ACTIVE_HIGH>;  
    };
};

&mipi_phy_rx0 {
	status = "okay";

	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port@0 {
			reg = <0>;
			#address-cells = <1>;
			#size-cells = <0>;

			mipi_in_ucam0: endpoint@1 {
				reg = <1>;
				remote-endpoint = <&ucam_out0>;
				data-lanes = <1 2>;
			};
		};

		port@1 {
			reg = <1>;
			#address-cells = <1>;
			#size-cells = <0>;

			dphy_rx_out: endpoint@0 {
				reg = <0>;
				remote-endpoint = <&isp_mipi_in>;
			};
		};
	};
};

&rkisp1 {
	status = "okay";
	port {
		#address-cells = <1>;
		#size-cells = <0>;

		isp_mipi_in: endpoint@0 {
			reg = <0>;
			remote-endpoint = <&dphy_rx_out>;
		};

	};
};

