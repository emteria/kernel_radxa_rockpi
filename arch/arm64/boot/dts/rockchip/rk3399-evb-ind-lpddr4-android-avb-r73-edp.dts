	#sound-dai-cells = <0>;
	status = "okay";
};
*/
//modify by pirlo for hdmi sound
&i2s2 {
	#sound-dai-cells = <0>;
	status = "disabled";
};

&hdmi_dp_sound {
	status = "disabled";
};


&route_hdmi {
        status = "disabled";
};
 
&pinctrl {
	lcd-panel {
		bl_en: bl-en {
			rockchip,pins = <1 2 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};
};

