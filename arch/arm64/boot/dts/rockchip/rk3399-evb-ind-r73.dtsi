/ {
	compatible = "rockchip,rk3399-evb-ind", "rockchip,rk3399";

	adc_keys {
		compatible = "adc-keys";
		io-channels = <&saradc 1>;
		io-channel-names = "buttons";
		keyup-threshold-microvolt = <1800000>;
		poll-interval = <100>;

		vol-up-key {
			label = "volume up";
			linux,code = <KEY_VOLUMEUP>;
			press-threshold-microvolt = <170000>;   //100000
		};

		vol-down-key {
			label = "volume down";
			linux,code = <KEY_VOLUMEDOWN>;
			press-threshold-microvolt = <270000>;
		};
	};

	gpio_keys: gpio-keys {
		status = "okay";
		compatible = "gpio-keys";
		autorepeat;
		pinctrl-names = "default";
		pinctrl-0 = <&pwrbtn>;
		pinctrl-1 = <&key_call1>;
		pinctrl-2 = <&key_call2>;

		power {
			debounce-interval = <100>;
			gpios = <&gpio0 5 GPIO_ACTIVE_LOW>;
			label = "GPIO Key Power";
			linux,code = <KEY_POWER>;
			wakeup-source;
		};
		
		call1 {
		    debounce-interval = <5>;
			gpios = <&gpio4 17 GPIO_ACTIVE_LOW>;
			linux,code = <289>;
			label = "GPIO Key call1";
			//pinctrl-names = "default", "gpio";
			//pinctrl-0 = <&i2c3_gpio>;
			gpio-key;
		};

		call2 {
		    debounce-interval = <5>;
			gpios = <&gpio4 16 GPIO_ACTIVE_LOW>;
			linux,code = <290>;
			label = "GPIO Key call2";
			//pinctrl-names = "default", "gpio";
			//pinctrl-0 = <&i2c3_gpio>;
			gpio-key;
	    };
	};

