	status = "disabled";
};

&dsi0_panel {
	status = "okay";
	power-supply = <&vcc_lcd>;
	enable-gpios = <&gpio3 RK_PD4 GPIO_ACTIVE_HIGH>;
	reset-gpios = <&gpio0 RK_PC6 GPIO_ACTIVE_LOW>;

	pinctrl-names = "default";
	pinctrl-0 = <&lcd_rst_gpio>;
};

