			native-mode = <&rgb_timing0>;
			
			rgb_timing0: timing0 {
				
				clock-frequency = <51000000>;
				hactive = <1024>;
				vactive = <600>;
				hback-porch = <150>;
				hfront-porch = <160>;
				vback-porch = <12>;
				vfront-porch = <12>;
				hsync-len = <4>;
				vsync-len = <6>;
				
				hsync-active = <0>;
				vsync-active = <0>;
				de-active = <0>;
				pixelclk-active = <0>;
				/*swap-rb = <0>;
				swap-rg = <0>;
				swap-gb = <0>;*/
			};
			
		};

		ports {
			rgb_panel_in: endpoint {
				remote-endpoint = <&rgb_out>;
			};
		};
	};
	
