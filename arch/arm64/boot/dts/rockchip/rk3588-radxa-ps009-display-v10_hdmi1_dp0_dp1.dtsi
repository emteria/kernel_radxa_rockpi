/************************************hdmi*******************************************/
&hdmi0 {
	status = "disabled";
};

&hdptxphy_hdmi0 {
	status = "disabled";
};

&hdmi0_in_vp0 {
	status = "disabled";
};

&hdmi1 {
	enable-gpios = <&gpio4 RK_PB1 GPIO_ACTIVE_HIGH>;
	status = "okay";
};

&hdptxphy_hdmi1 {
	status = "okay";
};

&hdmi1_in_vp1 {
	status = "okay";
};


/**********************************dp*********************************************/

&dp0 {
	status = "okay";
};

&dp0_in_vp2 {
	status = "okay";
};

&usbdp_phy0 {
	status = "okay";
};

&usbdp_phy0_dp {
	status = "okay";
};

&dp1 {
	pinctrl-names = "default";
	pinctrl-0 = <&dp1m0_pins>;
	status = "okay";
};

&dp1_in_vp0 {
	status = "okay";
};


&usbdp_phy1 {
	status = "okay";
	rockchip,dp-lane-mux = <2 3>;
};

&usbdp_phy1_dp {
	status = "okay";
};

/**********************************edp*********************************************/
&edp0 {
	force-hpd;
	disable-audio;
	status = "disabled";
	
	ports {
        port@1 {
            reg = <1>;

            edp0_out_panel: endpoint {
                remote-endpoint = <&panel_in_edp0>;
            };
        };
    };
};

&hdptxphy0 {
	status = "okay";
};
	
&hdptxphy_hdmi0 {
	status = "disabled";
};

&edp0_in_vp2 {
	status = "okay";
};

&vcc3v3_lcd_n {
	  compatible = "regulator-fixed";
	  regulator-name = "vcc3v3_lcd";
	  regulator-boot-on;
	  enable-active-high;
	  gpio = <&gpio3 RK_PA6 GPIO_ACTIVE_HIGH>;
};

&pwm8 {
	pinctrl-names = "active";
	pinctrl-0 = <&pwm8m0_pins>;
	status = "okay";
};
/**********************************dsi*********************************************/
//dsi1
&dsi1 {
	status = "disabled";
	rockchip,lane-rate = <480>;
	dsi1_panel: panel@0 {
		status = "okay";
	//	compatible ="radxa,display-8hd";
		compatible ="radxa,display-10hd-a";
		reg = <0>;
		backlight = <&dsi1_backlight>;

		vdd-supply = <&vcc_lcd_mipi1>;
		vccio-supply = <&vcc_1v8_s0>;
		reset-gpios = <&gpio4 RK_PC6 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&dsi1_lcd_rst_gpio>;
     	
		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@0 {
				reg = <0>;
				panel_in_dsi1: endpoint {
					remote-endpoint = <&dsi1_out_panel>;
				};
			};
		};
	};

	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port@1 {
			reg = <1>;
			dsi1_out_panel: endpoint {
				remote-endpoint = <&panel_in_dsi1>;
			};
		};
	};
};

&mipi_dcphy1 {
	status = "okay";
};

&dsi1_in_vp2 {
	status = "okay";
};

&pwm5 {
	status = "okay";
	pinctrl-names = "active";
	pinctrl-0 = <&pwm5m2_pins>;
};

//dsi0
&dsi0 {
	status = "disabled";
	rockchip,lane-rate = <480>;
	dsi0_panel: panel@0 {
		status = "okay";
	//	compatible ="radxa,display-8hd";
		compatible ="radxa,display-10hd-a";
		reg = <0>;
		backlight = <&dsi0_backlight>;

		vdd-supply = <&vcc_lcd_mipi1>;
		vccio-supply = <&vcc_1v8_s0>;
		reset-gpios = <&gpio2 RK_PC1 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&dsi0_lcd_rst_gpio>;

		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@0 {
				reg = <0>;
				panel_in_dsi0: endpoint {
					remote-endpoint = <&dsi0_out_panel>;
				};
			};
		};
	};

	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port@1 {
			reg = <1>;
			dsi0_out_panel: endpoint {
				remote-endpoint = <&panel_in_dsi0>;
			};
		};
	};
};

&mipi_dcphy0 {
	status = "okay";
};

&dsi0_in_vp3 {
	status = "okay";
};

&pwm2 {
	status = "okay";
	pinctrl-names = "active";
	pinctrl-0 = <&pwm2m2_pins>;
};

