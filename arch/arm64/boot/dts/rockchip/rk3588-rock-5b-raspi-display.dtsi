#include <dt-bindings/display/rockchip_vop.h>

/ {
	compatible = "radxa,rock-5b", "rockchip,rk3588";

	vcc_lcd_mipi1: vcc-lcd-mipi1 {
		status = "okay";
		compatible = "regulator-fixed";
		regulator-name = "vcc_lcd_mipi1";
		gpio = <&gpio1 RK_PC4 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		regulator-boot-on;
		regulator-always-on;
		regulator-state-mem {
			regulator-off-in-suspend;
		};
	};
};

&mipi_dcphy1 {
    status = "okay";
};

&dsi1 {
	status = "okay";
	#address-cells = <1>;
    #size-cells = <0>;
    rockchip,lane-rate = <605>;


	dsi1_panel: dsi-panel@0 {
		compatible = "raspits,tc358762-5inch";
		reg = <0x0>;
		status = "okay";

		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@0 {
				reg = <0>;

				panel_in_dsi1: endpoint {
					remote-endpoint = <&dsi1_out_panel>;
				};
			};
		};
	};

	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port@1 {
			reg = <1>;
			#address-cells = <1>;
			#size-cells = <0>;

			dsi1_out_panel: endpoint@0 {
				reg = <0>;
				remote-endpoint = <&panel_in_dsi1>;
			};
		};
	};
};

&dsi1_in_vp2 {
	status = "disabled";
};

&dsi1_in_vp3 {
	status = "okay";
};

&route_dsi1 {
	status = "okay";
	connect = <&vp3_out_dsi1>;
};

&i2c6 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&i2c6m0_xfer>;
	#address-cells = <1>;
	#size-cells = <0>;

    focaltech1: focaltech1@38 {
               status = "okay";
               compatible = "radxa,fts";
               reg = <0x38>;
               irq_mode = <0>; //0 poll ,1 irq
               x_reverse = <1>;
               y_reverse = <1>;
               xy_swap = <0>;
               focaltech,display-coords =  <0 0 800 480>;
               focaltech,max-touch-number = <5>;
     };

	rockpi_mcu: rockpi-mcu@45 {
		compatible = "rockpi_mcu";
		reg = <0x45>;
		status = "okay";
	};
};
