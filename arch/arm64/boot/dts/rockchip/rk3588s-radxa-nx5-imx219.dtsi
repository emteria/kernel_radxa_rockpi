&csi2_dcphy0 {
	status = "okay";

	ports {
		#address-cells = <1>;
		#size-cells = <0>;
		port@0 {
			reg = <0>;
			#address-cells = <1>;
			#size-cells = <0>;

			mipi_in_dcphy0: endpoint@2 {
				reg = <1>;
				remote-endpoint = <&imx219_out0>;
				data-lanes = <1 2>;
			};
		};

		port@1 {
			reg = <1>;
			#address-cells = <1>;
			#size-cells = <0>;

			csi_dcphy0_out: endpoint@0 {
				reg = <0>;
				remote-endpoint = <&mipi0_csi2_input>;
			};
		};
	};
};


&csi2_dphy2 {
	status = "okay";

	ports {
		#address-cells = <1>;
		#size-cells = <0>;
		port@0 {
			reg = <0>;
			#address-cells = <1>;
			#size-cells = <0>;

			mipi_in_dphy2: endpoint@1 {
				reg = <1>;
				remote-endpoint = <&imx219_out1>;
				data-lanes = <1 2>;
			};
		};
		port@1 {
			reg = <1>;
			#address-cells = <1>;
			#size-cells = <0>;

			csidcphy1_out: endpoint@0 {
				reg = <0>;
				remote-endpoint = <&mipi1_csi2_input>;
			};
		};
	};
};

&pinctrl {

	pinctrl-name = "default";
	pinctrl-0 = <&mipim0_camera0_clk>;
	
};

&i2c4 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&i2c4m2_xfer>;

	imx219: imx219@10 {
		compatible = "sony,imx219";
		reg = <0x10>;
		clocks = <&cru CLK_MIPI_CAMARAOUT_M0>;
		clock-names = "xvclk";
		power-domains = <&power RK3588_PD_VI>;
		pwdn-gpios = <&gpio1 RK_PA7 GPIO_ACTIVE_HIGH>;
		//reset-gpios = <&gpio4 RK_PB1 GPIO_ACTIVE_LOW>;
		//avdd-supply = <&vcc_mipidphy0>;
		rockchip,camera-module-index = <0>;
		rockchip,camera-module-facing = "back";
		rockchip,camera-module-name = "imx219";
		rockchip,camera-module-lens-name = "default";
		//lens-focus = <&cam_ircut0>;
		port {
			imx219_out0: endpoint {
				remote-endpoint = <&mipi_in_dcphy0>;
				data-lanes = <1 2>;
			};
		};
	};
};


&i2c6 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&i2c6m0_xfer>;

	imx219_1: imx219-1@10 {
		compatible = "sony,imx219";
		reg = <0x10>;
		clocks = <&cru CLK_MIPI_CAMARAOUT_M0>;
		clock-names = "xvclk";
		power-domains = <&power RK3588_PD_VI>;
		pwdn-gpios = <&gpio1 RK_PA6 GPIO_ACTIVE_HIGH>;
		//reset-gpios = <&gpio4 RK_PB1 GPIO_ACTIVE_LOW>;
		//avdd-supply = <&vcc_mipidphy0>;
		rockchip,camera-module-index = <1>;
		rockchip,camera-module-facing = "front";
		rockchip,camera-module-name = "imx219";
		rockchip,camera-module-lens-name = "default";
		//lens-focus = <&cam_ircut0>;
		port {
			imx219_out1: endpoint {
				remote-endpoint = <&mipi_in_dphy2>;
				data-lanes = <1 2>;
			};
		};
	};
};

